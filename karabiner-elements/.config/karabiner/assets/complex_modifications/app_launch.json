{
  "title": "Launch apps",
  "rules": [
    {
      "description": "AppLaunch: Launch VSCode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "w"
              },
              {
                "key_code": "e"
              }
            ],
            "modifiers": {
              "mandatory": [
                "option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/Applications/Visual Studio Code.app'"
            }
          ]
        }
      ]
    },
    {
